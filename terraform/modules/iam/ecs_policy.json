{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "dynamodb:GetRecords",
            "Resource": "arn:aws:dynamodb:*:*:table/* "
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "pricing:DescribeServices",
                "logs:CreateLogStream",
                "pricing:GetAttributeValues",
                "ecs:*",
                "logs:PutLogEvents",
                "logs:CreateLogGroup",
                "pricing:GetProducts"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObjectAcl",
                "dynamodb:BatchGetItem",
                "logs:CreateLogStream",
                "dynamodb:BatchWriteItem",
                "dynamodb:PutItem",
                "dynamodb:GetItem",
                "s3:CreateBucket",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:UpdateItem",
                "logs:CreateLogGroup",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:s3:::*",
                "arn:aws:logs:*:*:*",
                "arn:aws:dynamodb:*:*:table/*"
            ]
        }
    ]
}